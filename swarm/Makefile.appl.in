prefix = @prefix@
includedir = @includedir@
exec_prefix = @exec_prefix@
libdir = @libdir@

tcllibdir = @tcllibdir@
tklibdir = @tklibdir@
bltlibdir = @bltlibdir@
tclobjcdir = @tclobjcdir@
ffidir = @ffidir@
zliblibdir = @zliblibdir@
pnglibdir = @pnglibdir@

CC = @CC@
CFLAGS = @CFLAGS@

INCLUDES = -I$(includedir)

TCLLDFLAGS = @TCLLDFLAGS@
TKLDFLAGS = @TKLDFLAGS@
BLTLDFLAGS = @BLTLDFLAGS@
TCLOBJCLDFLAGS = @TCLOBJCLDFLAGS@
FFILDFLAGS = @FFILDFLAGS@
PNGLDFLAGS = @PNGLDFLAGS@
ZLIBLDFLAGS = @ZLIBLDFLAGS@

TCLLIB = @TCLLIB@
TKLIB = @TKLIB@
BLTLIB = @BLTLIB@
TCLOBJCLIB = @TCLOBJCLIB@
FFILIB = @FFILIB@

PNGLIB = -lpng
ZLIBLIB = -lz

X_LIBS = @X_LIBS@
X_EXTRA_LIBS = @X_EXTRA_LIBS@
GUILIBS = @GUILIBS@

LDFLAGS = -L$(libdir) @RPATH@$(libdir) $(TCLOBJCLDFLAGS) $(BLTLDFLAGS) $(TCLLDFLAGS) $(TKLDFLAGS) $(FFILDFLAGS) $(PNGLDFLAGS) $(ZLIBLDFLAGS) $(X_LIBS)
#LINK = libtool --mode link $(CC) $(CFLAGS) $(LDFLAGS) -o $@
LINK = $(CC) $(CFLAGS) $(LDFLAGS) -o $@

SYSLIBS = $(TCLOBJCLIB) $(BLTLIB) $(TKLIB) $(TCLLIB) $(PNGLIB) $(ZLIBLIB) $(FFILIB) $(GUILIBS) -lm

SWARMLIBS = -lspace -lanalysis -lsimtools -lsimtoolsgui -ltkobjc -lactivity -lrandom -lobjectbase -lcollections -ldefobj -lmisc

LIBS = @LIBS@

all: $(APPLICATION)

clean:
	-rm -f $(OBJECTS) $(APPLICATION) $(OTHERCLEAN) core gmon.out

$(APPLICATION): $(OBJECTS)
	$(LINK) $(OBJECTS) $(APPLIBS) $(SWARMLIBS) $(SYSLIBS) $(LIBS)

.SUFFIXES: .m .xm

.m.o:
	$(CC) -c -Wno-import $(CFLAGS) $(INCLUDES) $<   

