include $(top_builddir)/etc/Makefile.common

INCLUDES = $(foreach module,. $(SWARMMODULES),-I$(top_srcdir)/src/$(module) -I$(top_builddir)/src/$(module)) -I$(top_srcdir)/src/gui/

builddir = $(shell cd ../../src; pwd)
libobjcdir = $(shell cd ../../libobjc; pwd)
javaswarmdir = $(shell cd ../../java/c; pwd)

JAVASWARMLIBS = @JAVASWARMLIBS@

SWARMBUILDLDFLAGS = -L$(javaswarmdir) -R $(javaswarmdir) $(foreach module,$(SWARMMODULES),-L$(builddir)/$(module) -R $(builddir)/$(module)) -L$(libobjcdir) -R $(libobjcdir) 

LDFLAGS = $(SWARMBUILDLDFLAGS) $(SUPPORTLDFLAGS)

LIBS = $(JAVASWARMLIBS) $(SWARMLIBS) $(SYSLIBS) @LIBS@

# Local Variables:
# mode: makefile
# End:
