## Config file for applications - this is processed by `configure'
## at `installation-time' to generate a Swarm configuration file.
CC="@CC@"

## Anciliary variables that need to be set in order for the final
## (substituted) SWARM_LD{FLAGS,ADD} variable to be set correctly
swarm_prefix="@swarm_prefix@"
swarm_exec_prefix="@swarm_exec_prefix@"
swarm_libdir="@swarm_libdir@"

tcllibdir="@tcllibdir@"
tklibdir="@tklibdir@"
bltlibdir="@bltlibdir@"
ffidir="@ffidir@"
zliblibdir="@zliblibdir@"
pnglibdir="@pnglibdir@"

swarm_SWARMLDFLAGS="@SWARMLDFLAGS@"
swarm_TCLLDFLAGS="@TCLLDFLAGS@"
swarm_TKLDFLAGS="@TKLDFLAGS@"
swarm_BLTLDFLAGS="@BLTLDFLAGS@"
swarm_FFILDFLAGS="@FFILDFLAGS@"
swarm_PNGLDFLAGS="@PNGLDFLAGS@"
swarm_ZLIBLDFLAGS="@ZLIBLDFLAGS@"

swarm_TCLLIB="@TCLLIB@"
swarm_TKLIB="@TKLIB@"
swarm_BLTLIB="@BLTLIB@"
swarm_FFILIB="@FFILIB@"

swarm_PNGLIB="-lpng"
swarm_ZLIBLIB="-lz"

swarm_X_LIBS="@X_LIBS@"
swarm_GUILIBS="@GUILIBS@"

swarm_DLLIB="@DLLIB@"
swarm_OBJCLIBS="@OBJCLIBS@"

swarm_SYSLIBS="${swarm_BLTLIB} ${swarm_TKLIB} ${swarm_TCLLIB} ${swarm_PNGLIB} ${swarm_ZLIBLIB} ${swarm_FFILIB} ${swarm_GUILIBS} -lm ${swarm_OBJCLIBS} ${swarm_DLLIB}"

swarm_SWARMLIBS="-lspace -lanalysis -lsimtools -lsimtoolsgui -ltkobjc -ltclobjc -lactivity -lrandom -lobjectbase -lcollections -ldefobj -lmisc"

swarm_LIBS="@LIBS@"

## The final shell variables which are to be substituted in the application
## Makefiles(s)

SWARM_LDFLAGS="${swarm_SWARMLDFLAGS} ${swarm_BLTLDFLAGS} ${swarm_TCLLDFLAGS} ${swarm_TKLDFLAGS} ${swarm_FFILDFLAGS} ${swarm_PNGLDFLAGS} ${swarm_ZLIBLDFLAGS} ${swarm_X_LIBS}"

SWARM_LDADD="${swarm_SWARMLIBS} ${swarm_SYSLIBS} ${swarm_LIBS}"
