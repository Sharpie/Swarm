prefix = @prefix@
includedir = @includedir@
exec_prefix = @exec_prefix@
libdir = @libdir@
bindir = @bindir@
datadir = @datadir@
swarmdatadir = $(datadir)/swarm
sysconfdir = @sysconfdir@
swarmsysconfdir = $(sysconfdir)/swarm

swarm_prefix=@swarm_prefix@
swarm_exec_prefix=@swarm_exec_prefix@
swarm_libdir=@swarm_libdir@

xpmlibdir = @xpmlibdir@
tcllibdir = @tcllibdir@
tklibdir = @tklibdir@
bltlibdir = @bltlibdir@
ffilibdir = @ffilibdir@
zliblibdir = @zliblibdir@
pnglibdir = @pnglibdir@
hdf5libdir = @hdf5libdir@

CC = @CC@
CFLAGS = @CFLAGS@
CPPFLAGS = @CPPFLAGS@
EXPORT_EXTERN = @EXPORT_EXTERN@
IMPORT_EXTERN = @IMPORT_EXTERN@

OBJC=@OBJC@
OBJCFLAGS=@OBJCFLAGS@ $(EXTRAOBJCFLAGS)

SWARMINCLUDES = -I$(includedir)

SWARMLDFLAGS = @SWARMLDFLAGS@
XPMLDFLAGS = @XPMLDFLAGS@
TCLLDFLAGS = @TCLLDFLAGS@
TKLDFLAGS = @TKLDFLAGS@
BLTLDFLAGS = @BLTLDFLAGS@
FFILDFLAGS = @FFILDFLAGS@
PNGLDFLAGS = @PNGLDFLAGS@
ZLIBLDFLAGS = @ZLIBLDFLAGS@
HDF5LDFLAGS = @HDF5LDFLAGS@

ifeq ($(USE_EXTRALDFLAGS),)
EXTRALDFLAGS =
else
EXTRALDFLAGS = @EXTRALDFLAGS@
endif

INSTALL = @INSTALL@
INSTALL_PROGRAM = @SWARM_INSTALL_PROGRAM@
INSTALL_DATA = @SWARM_INSTALL_DATA_@

XPMLIB = @XPMLIB@
TCLLIB = @TCLLIB@
TKLIB = @TKLIB@
BLTLIB = @BLTLIB@
FFILIB = @FFILIB@
HDF5LIB = @HDF5LIB@

PNGLIB = @PNGLIB@
ZLIBLIB = @ZLIBLIB@

X_LIBS = @X_LIBS@
GUILIBS = @GUILIBS@

DLLIB = @DLLIB@

SUPPORTLDFLAGS = $(HDF5LDFLAGS) $(FFILDFLAGS) $(PNGLDFLAGS) $(ZLIBLDFLAGS) $(BLTLDFLAGS) $(TCLLDFLAGS) $(XPMLDFLAGS) $(TKLDFLAGS) $(X_LIBS)

SYSLIBS = $(BLTLIB) $(TKLIB) $(TCLLIB) $(XPMLIB) $(PNGLIB) $(HDF5LIB) $(ZLIBLIB) $(FFILIB) $(GUILIBS) -lm $(DLLIB)

SWARMMODULES = space analysis simtoolsgui simtools random tkobjc tclobjc \
	objectbase activity defobj collections misc


USEDLL=@USEDLL@

ifeq ($(USEDLL),yes)
ifneq ($(USESTATIC),)
SWARMLIBS = -lswarm
OBJCLIBS = -lobjc
else
SWARMLIBS = -lswarmdll -lobjcdll
OBJCLIBS = 
DLLCPPFLAGS = -DDLL
endif
else
SWARMLIBS = -lswarm
OBJCLIBS = -lobjc
endif

