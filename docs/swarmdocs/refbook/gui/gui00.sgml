  <SECT1 ID="SWARM.GUI.SGML.SECT1.COMPAT">
    <TITLE>Compatibility</TITLE>

    <ITEMIZEDLIST>

      <LISTITEM>
        <FORMALPARA>
          <TITLE>1.0.5 -&gt; 1.1</TITLE> 

          <PARA>This library was created with the Swarm 1.1 release,
          earlier version of Swarm used the tkobjc functions
          directly.</PARA>

        </FORMALPARA>
      </LISTITEM>

    </itemizedlist>
    
  </SECT1>

