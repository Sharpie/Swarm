<sect2>
 <title>PPT Slide</title> 

    <para>What these classes do</para>

    <itemizedlist>
      
      <listitem><formalpara><title>Activity</title><para> Schedules,
        ActivityGroups and Swarms</para> </formalpara></listitem>

      <listitem><formalpara><title>Probe </title><para> Probing
            object∆s state and behavior</para> </formalpara></listitem>

      <listitem><formalpara><title>Random </title><para> Random number
            generators and distributions</para> </formalpara></listitem>

      <listitem><formalpara><title>Simtools</title><para> Grab bag of
            various tools</para> </formalpara></listitem>

      <listitem<formalpara><title>Collection </title><para>Arrays,
            Lists and ordered Lists of objects</para>
        </formalpara>
      </listitem>

      <listitem><formalpara><title>GUI </title><para>Graphical
            interface objects</para>
        </formalpara>
      </listitem>

      <listitem><formalpara><title>Space </title><para>Manages
            information for 2D lattice or grids</para>
        </formalpara>
      </listitem>

      <listitem><formalpara><title>Analysis </title><para>Easier
            interface to graphs, calculate statistics</para>
        </formalpara>
      </listitem>
    </itemizedlist>

</sect2>

<sect2>
 <title>Collection</title> 

    <formalpara><title>List</title><para>A flat list of objects with
        random access </para>
    </formalpara>

    <formalpara><title>KeyedCollection</title><para>
        <itemizedlist>
          <listitem>
            <formalpara><title>Map </title><para>Key  is independent object</para>
            </formalpara>
          </listitem>

          <listitem>
            <formalpara><title>Set </title><para>Key is the member itself </para>
            </formalpara>
          </listitem>
        </itemizedlist>
      </para></formalpara>

    <formalpara><title>Array</title><para>
        Collection with fixed size</para></formalpara>

    <literallayout>Collection
List
Keyed
Collection
Array
Map
Set
</literallayout>

</sect2>
