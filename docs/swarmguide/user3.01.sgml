<sect1 id="swarm.user.user3.01.sect1">
  <title>Introduction to the Libraries</title>

<sect2>
 <title>Swarm Libraries</title> 

    <formalpara><title>More on Swarm libraries</title><para>

        <itemizedlist>

          <listitem>
            <formalpara><title>Collection</title>
              <para>Lists, Arrays, Sorted lists</para>
            </formalpara></listitem> 

          <listitem>
            <formalpara><title>Random</title>
              <para>Random numbers</para></formalpara></listitem>

          <listitem><formalpara><title>Analysis</title>
              <para>Graphs,Histograms and data filters</para>
            </formalpara></listitem>

          <listitem><formalpara><title>Space</title>
              <para>Latticeclasses</para>
            </formalpara></listitem>

          <listitem><formalpara><title>Activity</title>
              <para>Schedulesand ActivityGroups</para>
            </formalpara></listitem>
        </itemizedlist>

      </para></formalpara> 

</sect2>

